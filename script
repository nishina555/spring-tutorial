# CREATE ROLE
CREATE ROLE com LOGIN
ENCRYPTED PASSWORD 'md586082399b5082acb54472ee195a57ce8'
NOSUPERUSER INHERIT NOCREATEDB NOCREATEROLE NOREPLICATION;

# CREATE DATABASE
CREATE DATABASE com
WITH OWNER = com
ENCODING = 'UTF8'
TABLESPACE = pg_default
LC_COLLATE = 'C'
LC_CTYPE = 'C'
TEMPLATE = 'template0'
CONNECTION LIMIT = -1;
